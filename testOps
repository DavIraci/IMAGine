// Working comparison operators

list temp={};

if(5>3.3 && 1==1 && (5>20 || 1==1))then{
	print(">");
};

if(3<"ciao")then{
	print("<");
};

if(5>=5)then{
	print(">=");
};

if(5<=5)then{
	print("<=");
};

if(5==5)then{
	print("==");
};

val=(5>3);
print(val);

