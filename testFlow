// Working conditional statements and flows

counter=0;

while(counter<5)do{
	print(counter);
	counter=counter+1;
}

if(5>6)then{
	print("IF");
}else{
	print("ELSE");
}


while(counter<50)do{
	if(counter==8)then{
		print("COUNTER = "+counter);
	};
	counter=counter+1;
}

list li = {1,2,3};

foreach(temp:li){
	temp=temp+1;
	print(temp);
}

print("------");
print(li);

// -------------------------------------------


