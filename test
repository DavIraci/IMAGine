img im="/home/kali/Desktop/a.jpeg";

out="/home/kali/Desktop/cropped.jpg";
out2="/home/kali/Desktop/inverted.jpg";
out3="/home/kali/Desktop/rotate.jpg";
out4="/home/kali/Desktop/suca.jpg";

print(width(im));
print(height(im));
print(bands(im));

img cropped=crop(im,out, 25, 25, 25, 25);
